{
  "homepage": "https://marcelosperalta.github.io/fullstack_mockup_ElearningPlatform/",
  "name": "rocketseat_nlw2",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "clean": "del-cli dist",
    "publish": "gh-pages -d dist",
    "copy-win": "mkdir dist && xcopy src dist /e /exclude:excludelist.txt",
    "copy-linux": "mkdir dist && rsync -avr --exclude=\"/scss\" src/ dist",
    "deploy-windows": "run-s clean copy-win publish",
    "deploy-linux": "run-s copy-linux publish"
  },
  "dependencies": {
    "del-cli": "^3.0.1",
    "express": "^4.17.1",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.2",
    "sqlite-async": "^1.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
